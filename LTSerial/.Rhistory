R_Serial <- function (DEV_ADDR, Delay){
Raw_data <- ""
i<-0
stopTime <- Delay/10+5
while (i < stopTime)
{
newText <- read.serialConnection(DEV_ADDR)
if(0 < nchar(newText))
{
Raw_data <- paste(Raw_data, newText)
}
Sys.sleep(10)
i = i+1
}
return(Raw_data)
}
