RW_Serial <- function (DEV_ADDR, CMD){
read.serialConnection(DEV_ADDR)
write.serialConnection(DEV_ADDR, CMD)
stopTime <- Sys.time() + 5
while(Sys.time() < stopTime)
{
data<-read.serialConnection(DEV_ADDR)
if(nchar(data)>0){
send <- as.numeric(data)
break
}
else{
send <- 0
}
}
return(send)
}
